{
  "tabWidth": 2,
  "printWidth": 80,
  "proseWrap": "never",
  "trailingComma": "all",
  "singleQuote": true,
  "plugins": [
    "prettier-plugin-organize-imports",
    "prettier-plugin-tailwindcss"
  ],
  "overrides": [
    {
      "files": ["*.json"],
      "options": {
        "printWidth": 256
      }
    }
  ]
}
